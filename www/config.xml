<?xml version='1.0' encoding='utf-8'?>
<widget id="edu.ucdenver.cbb1_0" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>CBB1.0</name>
    <description>
        A sample web and mobile application for the Bright Beginnings Program.
    </description>
    <author email="agileehealth@gmail.com" href="brightbeginningsusa.org">
        Sheana Bull
    </author>
    <feature name="http://api.phonegap.com/1.0/device" />

    <feature name="InAppBrowser">
        <param name="android-package" value="org.apache.cordova.InAppBrowser" />
    </feature>
    <feature name="InAppBrowser">
        <param name="ios-package" value="CDVInAppBrowser" />
    </feature>

    <preference name="permissions" value="none" />
    <preference name="orientation" value="default" />
    <preference name="target-device" value="universal" />
    <preference name="fullscreen" value="true" />
    <preference name="webviewbounce" value="true" />
    <preference name="prerendered-icon" value="true" />
    <preference name="stay-in-webview" value="false" />
    <preference name="ios-statusbarstyle" value="black-opaque" />
    <preference name="detect-data-types" value="true" />
    <preference name="exit-on-suspend" value="false" />
    <preference name="show-splash-screen-spinner" value="true" />
    <preference name="auto-hide-splash-screen" value="true" />
    <preference name="disable-cursor" value="false" />
    <preference name="android-minSdkVersion" value="7" />
    <preference name="android-installLocation" value="auto" />
    <icon src="img/bb-logo.png" />
    <icon gap:density="ldpi" gap:platform="android" src="res/icon/android/bb-logo36x36.png" />
    <icon gap:density="mdpi" gap:platform="android" src="res/icon/android/bb-logo48x48.png" />
    <icon gap:density="hdpi" gap:platform="android" src="res/icon/android/bb-logo72x72.png" />
    <icon gap:density="xhdpi" gap:platform="android" src="res/icon/android/bb-logo96x96.png" />
    <icon gap:platform="blackberry" src="res/icon/blackberry/icon-80.png" />
    <icon gap:platform="blackberry" gap:state="hover" src="res/icon/blackberry/icon-80.png" />
    <icon gap:platform="ios" height="57" src="res/icon/ios/bb-logo57.png" width="57" />
    <icon gap:platform="ios" height="72" src="res/icon/ios/bb-logo72.png" width="72" />
    <icon gap:platform="ios" height="114" src="res/icon/ios/bb-logo114.png" width="114" />
    <icon gap:platform="ios" height="144" src="res/icon/ios/bb-logo144.png" width="144" />
    <icon gap:platform="webos" src="res/icon/webos/icon-64.png" />
    <icon gap:platform="winphone" src="res/icon/windows-phone/icon-48.png" />
    <icon gap:platform="winphone" gap:role="background" src="res/icon/windows-phone/icon-173.png" />
    <gap:splash gap:density="ldpi" gap:platform="android" src="res/screen/android/bb-logo200x320.png" />
    <gap:splash gap:density="mdpi" gap:platform="android" src="res/screen/android/bb-logo320x480.png" />
    <gap:splash gap:density="hdpi" gap:platform="android" src="res/screen/android/bb-logo480x800.png" />
    <gap:splash gap:density="xhdpi" gap:platform="android" src="res/screen/android/bb-logo720x1280.png" />
    <gap:splash gap:platform="blackberry" src="res/screen/blackberry/screen-225.png" />
    <gap:splash gap:platform="ios" height="480" src="res/screen/ios/bb-logo320x480.png" width="320" />
    <gap:splash gap:platform="ios" height="960" src="res/screen/ios/bb-logo_640x960.png" width="640" />
    <gap:splash gap:platform="ios" height="1024" src="res/screen/ios/bb-logo768x1024.png" width="768" />
    <gap:splash gap:platform="ios" height="768" src="res/screen/ios/bb-logo1024x768.png" width="1024" />
    <gap:splash gap:platform="winphone" src="res/screen/windows-phone/screen-portrait.jpg" />
    <gap:plugin name="com.phonegap.plugins.PushPlugin" />
    <access origin="*" />
    
</widget>
